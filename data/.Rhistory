students2014$deep <- rowMeans(deep_columns)
surface_columns <- select(students2014, one_of(surface_questions))
students2014$surf <- rowMeans(surface_columns)
strategic_columns <- select(students2014, one_of(strategic_questions))
students2014$stra <- rowMeans(strategic_columns)
